<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="Adv.Domain" namespace="Adv.Domain.NHibernate">
  <class name="ProductCategory" table="Production.ProductCategory" dynamic-update="true" >
    <cache usage="read-write" />
    <id name="Id" column="ProductCategoryID" type="int">
      <generator class="native" />
    </id>
    <property name="ProductCategoryName" column="Name" />
    <property name="rowguid" />
    <property name="ModifiedDate" />
    
  </class>
</hibernate-mapping>